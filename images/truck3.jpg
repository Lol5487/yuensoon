<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 800 500">
  <!-- 背景 -->
  <rect width="800" height="500" fill="#f5f5f5"/>
  
  <!-- 天空 -->
  <rect width="800" height="250" fill="#87CEEB"/>
  
  <!-- 地面 -->
  <rect y="250" width="800" height="250" fill="#555"/>
  
  <!-- 平板卡车 -->
  <g transform="translate(150, 300)">
    <!-- 驾驶室 -->
    <rect x="0" y="-80" width="120" height="80" rx="10" fill="#0056b3"/>
    <rect x="20" y="-60" width="80" height="40" fill="#87CEEB" opacity="0.8"/>
    
    <!-- 平板 -->
    <rect x="120" y="-40" width="400" height="20" fill="#333"/>
    <rect x="120" y="-60" width="400" height="20" fill="#444"/>
    
    <!-- 货物 -->
    <rect x="200" y="-120" width="200" height="60" fill="#8B4513"/>
    <rect x="210" y="-110" width="180" height="50" fill="#A0522D" stroke="#333" stroke-width="1"/>
    
    <!-- 公司标志 -->
    <rect x="250" y="-90" width="100" height="30" rx="5" fill="#fff"/>
    <text x="260" y="-70" font-family="Arial" font-size="14" fill="#0056b3">YUEN SOON</text>
    
    <!-- 轮子 -->
    <circle cx="60" cy="0" r="30" fill="#333"/>
    <circle cx="60" cy="0" r="15" fill="#666"/>
    
    <circle cx="200" cy="0" r="30" fill="#333"/>
    <circle cx="200" cy="0" r="15" fill="#666"/>
    
    <circle cx="300" cy="0" r="30" fill="#333"/>
    <circle cx="300" cy="0" r="15" fill="#666"/>
    
    <circle cx="400" cy="0" r="30" fill="#333"/>
    <circle cx="400" cy="0" r="15" fill="#666"/>
  </g>
</svg>